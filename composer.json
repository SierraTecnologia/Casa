{
    "name": "sierratecnologia/casa",
    "description": "casa",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "casa"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/casa/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/casa/issues",
        "source": "https://github.com/sierratecnologia/casa",
        "docs": "https://github.com/sierratecnologia/casa/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "sierratecnologia/integrations": "^0.4.0",
        "sierratecnologia/population": "^0.4.0",
        "sierratecnologia/telefonica": "^0.4.0",
        "sierratecnologia/bancario": "^0.4.0",
        "sierratecnologia/transmissor": "^0.4.0",
        "sierratecnologia/payment-laravel": "^0.3.0 | ^0.4.0",
        "spatie/laravel-google-calendar": "*"
    },
    "suggest": {
        "sierratecnologia/stalker": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Casa\\": "src/"
        }
    },
    "bin": ["bin/casa-sincronizador"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": [],
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Casa\\CasaProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    },
    "require-dev": {
        "phpro/grumphp": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "php-http/discovery": true
        }
    }
}
