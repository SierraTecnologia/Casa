{
    "name": "sierratecnologia/casa",
    "description": "casa",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": ">=7.0",
        "overtrue/laravel-follow": ">=1.1",
        "sierratecnologia/payment-laravel": "dev-master",
        "sierratecnologia/finder": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Casa\\": "src/"
        }
    },
    "bin": ["bin/casa-sincronizador"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Casa\\CasaProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
